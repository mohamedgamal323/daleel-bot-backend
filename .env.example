# Example environment configuration for Codex
LLM_PROVIDER=openai
VECTOR_DB=qdrant
OPENAI_API_KEY=your-openai-api-key
# Optional custom base URL for OpenAI
OPENAI_API_BASE=
# Default model names
OPENAI_MODEL=gpt-3.5-turbo
OPENAI_EMBED_MODEL=text-embedding-ada-002
# Qdrant connection settings
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=
QDRANT_COLLECTION=assets

# Repository type: "mongodb" or "memory"
REPOSITORY_TYPE=mongodb
# MongoDB connection settings
MONGODB_URL=mongodb://localhost:27007
MONGODB_DATABASE=daleel_bot
